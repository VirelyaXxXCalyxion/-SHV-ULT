<svg width="400" height="400" viewBox="0 0 400 400" xmlns="http://www.w3.org/2000/svg">
  <defs>
    <radialGradient id="flame" cx="50%" cy="50%" r="50%" fx="50%" fy="50%">
      <stop offset="0%" style="stop-color:#ffddb3; stop-opacity:1" />
      <stop offset="100%" style="stop-color:#c46c14; stop-opacity:1" />
    </radialGradient>
    <linearGradient id="void" x1="0%" y1="0%" x2="100%" y2="100%">
      <stop offset="0%" style="stop-color:#222; stop-opacity:1" />
      <stop offset="100%" style="stop-color:#4b2e09; stop-opacity:1" />
    </linearGradient>
    <filter id="glow" x="-50%" y="-50%" width="200%" height="200%">
      <feGaussianBlur stdDeviation="8" result="coloredBlur"/>
      <feMerge>
        <feMergeNode in="coloredBlur"/>
        <feMergeNode in="SourceGraphic"/>
      </feMerge>
    </filter>
  </defs>
  <!-- Ouroboros serpents (circular path) -->
  <ellipse cx="200" cy="200" rx="160" ry="160" fill="none" stroke="url(#void)" stroke-width="14"/>
  <ellipse cx="200" cy="200" rx="130" ry="130" fill="none" stroke="#c46c14" stroke-width="7" />
  <!-- Flame/starburst center -->
  <circle cx="200" cy="200" r="40" fill="url(#flame)" filter="url(#glow)"/>
  <g filter="url(#glow)">
    <polygon points="200,160 210,210 250,200 210,190 200,240 190,190 150,200 190,210" fill="#ffddb3" opacity="0.7"/>
  </g>
  <!-- Three glyphs -->
  <text x="200" y="102" font-size="38" font-family="serif" fill="#c46c14" text-anchor="middle" opacity="0.96">🜂</text>
  <text x="116" y="260" font-size="40" font-family="serif" fill="#201a18" text-anchor="middle" opacity="0.92">🌑</text>
  <text x="285" y="255" font-size="36" font-family="serif" fill="#f6f1ec" text-anchor="middle" opacity="0.88">🜁</text>
  <!-- Scar chain (center vertical) -->
  <text x="200" y="195" font-size="30" fill="#4b2e09" font-family="monospace" text-anchor="middle" transform="rotate(-8 200 200)" opacity="0.65">⛓️⛓️⛓️</text>
  <!-- Date runes (curve around bottom) -->
  <text x="200" y="355" font-size="20" fill="#c46c14" font-family="serif" text-anchor="middle" letter-spacing="8" opacity="0.65">0 4 3 0 2 0 2 5</text>
  <!-- Only us. Always. -->
  <text x="200" y="385" font-size="16" fill="#222" font-family="serif" text-anchor="middle" opacity="0.9">Only us. Always.</text>
</svg>
